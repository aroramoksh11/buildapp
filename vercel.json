{
  "version": 2,
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/sw.js",
      "dest": "/sw.js",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "service-worker-allowed": "/"
      },
      "continue": true
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "content-type": "application/manifest+json"
      },
      "continue": true
    },
    {
      "handle": "miss"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 