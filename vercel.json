{
  "version": 2,
  "builds": [
    {
      "src": "public/sw.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "service-worker-allowed": "/"
      },
      "continue": true
    },
    {
      "src": "/manifest.json",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "content-type": "application/manifest+json"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 